
<div class = "track-upload-container">
  <h4>Upload Track</h4>

  <%= form_for @track, html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :artist %>
      <%= f.text_field :artist %>
      <%= f.label :url1 %>
      <%= f.text_field :channel1 %>
      <%= f.label :url2 %>
      <%= f.text_field :channel2 %>
      <%= f.label :url3 %>
      <%= f.text_field :channel3 %>
      <%= f.label :url4 %>
      <%= f.text_field :channel4 %>
      <%= f.label :url5 %>
      <%= f.text_field :channel5 %>
      <br>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
</div>
